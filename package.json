    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux-logger": "^3.0.6"
  }
}

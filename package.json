{
  "name": "gallery",
  "version": "1.0.0",
  "description": "just simple RWD page",
  "main": "./src/index.jsx",
  "scripts": {
    "test": "jest",
    "testing": "nodemon --watch  -e jsx  --exec \"npm  test \" ",
    "build": "webpack --config webpack.prod.js --env production",
    "start": "  nodemon --watch  webpack.dev.js --exec \" webpack-dev-server --config webpack.dev.js --content-base dist/  --env development\"  ",
    "server": "nodemon server.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "transform": {
      ".*": "<rootDir>node_modules/babel-jest"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/",
      "node_modules/redux/"
    ],
    "collectCoverageFrom": [
      "./src/components/**/test/test.jsx",
      "./src/components/**/test/*.jsx",
      "./src/components/*.test.jsx",
      "!./src/components/**/*.scss",
      "!./src/components/**/*.png",
      "!./src/components/**/*.jpg"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SerhiiBilyk/gallery.git"
  },
  "author": "SerhiiBilyk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SerhiiBilyk/gallery/issues"
  },
  "homepage": "https://github.com/SerhiiBilyk/gallery#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-react-css-modules": "^3.3.1",
    "babel-plugin-styled-components": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-jest": "^21.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "compression-webpack-plugin": "^1.0.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.0",
    "lodash-webpack-plugin": "^0.11.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^2.0.6",
    "postcss-plugin": "^1.0.0",
    "react-css-modules": "^4.7.1",
    "react-test-renderer": "^16.0.0",
    "redux-devtools": "^3.4.1",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "style-ext-html-webpack-plugin": "^3.4.4",
    "style-loader": "^0.18.2",
    "styled-components": "^2.2.0",
    "ts-loader": "^3.1.1",
    "typescript": "^2.6.1",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.1",
